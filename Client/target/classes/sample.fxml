<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="450.0" prefWidth="700.0">
         <children>
            <ListView fx:id="clientFilesList" layoutX="14.0" layoutY="43.0" prefHeight="310.0" prefWidth="311.0" />
            <ListView fx:id="serverFilesList" layoutX="374.0" layoutY="43.0" prefHeight="310.0" prefWidth="311.0" />
            <Button fx:id="upload" layoutX="338.0" layoutY="62.0" mnemonicParsing="false" onAction="#uploadFile" text="&gt;" />
            <Button fx:id="download" layoutX="338.0" layoutY="106.0" mnemonicParsing="false" onAction="#downloadFile" text="&lt;" />
            <Label layoutX="68.0" layoutY="14.0" prefHeight="27.0" prefWidth="202.0" text="Your files in a computer">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Label layoutX="446.0" layoutY="14.0" prefHeight="27.0" prefWidth="166.0" text="Your files in a server" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <TextField fx:id="login" layoutX="21.0" layoutY="391.0" prefHeight="26.0" prefWidth="120.0" />
            <PasswordField fx:id="password" layoutX="149.0" layoutY="390.0" prefHeight="26.0" prefWidth="120.0" />
            <Label layoutX="59.0" layoutY="356.0" text="Login">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Label layoutX="171.0" layoutY="356.0" text="Password">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Button fx:id="enter" layoutX="278.0" layoutY="390.0" mnemonicParsing="false" onAction="#loginUser" prefHeight="26.0" prefWidth="67.0" text="Enter" />
            <Label fx:id="msg" layoutX="434.0" layoutY="381.0" prefHeight="46.0" prefWidth="251.0" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Button fx:id="logout" disable="true" layoutX="355.0" layoutY="390.0" mnemonicParsing="false" onAction="#logOut" prefHeight="26.0" prefWidth="67.0" text="Logout" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
